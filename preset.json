{
  "bare": true,
  "useConfigFiles": true,
  "plugins": {
    "@vue/cli-plugin-babel": {},
    "@vue/cli-plugin-eslint": {
      "config": "prettier",
      "lintOn": [
        "save"
      ]
    },
    "@vue/cli-plugin-router": {
      "historyMode": true
    },
    "@vue/cli-plugin-vuex": {},
    "vue-cli-plugin-i18n": {
      "locale": "en",
      "fallbackLocale": "en",
      "localeDir": "locales",
      "enableInSFC": "false"
    },
    "vue-cli-plugin-vuetify": {
      "preset": "configure",
      "replaceComponents": false,
      "useTheme": false,
      "useCustomProperties": false,
      "iconFont": "mdi",
      "installFonts": false,
      "useAlaCarte": true,
      "usePolyfill": true,
      "locale": "en"
    },
    "vue-cli-plugin-vtec-admin": {
      "prompts": true
    }
  },
  "cssPreprocessor": "dart-sass",
  "configs": {
    "vue": {
      "devServer": {
        "disableHostCheck": true
      }
    }
  }
}
